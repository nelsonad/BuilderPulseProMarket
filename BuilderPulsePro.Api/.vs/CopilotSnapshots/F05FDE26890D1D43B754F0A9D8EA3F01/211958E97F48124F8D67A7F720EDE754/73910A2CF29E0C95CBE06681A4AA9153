namespace BuilderPulsePro.Api.Contracts;

public record ContractorProfileResponse(
    string DisplayName,
    string[] Trades,
    double Lat,
    double Lng,
    int ServiceRadiusMeters,
    DateTimeOffset UpdatedAt
);

public record UpsertContractorProfileRequest(
    string DisplayName,
    string[] Trades,
    double Lat,
    double Lng,
    int ServiceRadiusMeters
);
